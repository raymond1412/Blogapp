<h1>Log In</h1>
<%= form_tag sessions_path do %>

<p><div class="field">
   <%= label_tag :email %>
   <%= text_field_tag :email %>
</div></p>

 <p><div class="field">
   <%= label_tag :password %>
   <%= password_field_tag :password %>
</div></p>

 <p><div class="actions">
    <%= submit_tag "Log In!" %>
</div></P>

<%= link_to 'New User', new_user_path %>
<%= link_to 'Back', users_path %>
<% end %>